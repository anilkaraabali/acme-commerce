    "merge-products": "./scripts/merge-products/run.sh",
    "merge-products:debug": "node ./scripts/merge-products/script.js --debug",
    "pre:build": "pnpm pre",
    "pre": "pnpm copy-pdf-worker && pnpm merge-products",
    "pre:dev": "pnpm pre",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage --silent",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
